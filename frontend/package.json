{
  "name": "mini-game",
  "version": "0.0.5",
  "description": "Toying around with TypeScript",
  "keywords": [
    "Phaser",
    "TypeScript",
    "Spine",
    "MatterJS",
    "Parcel"
  ],
  "browserslist": [
    "defaults",
    "> 0.5%",
    "not IE 11"
  ],
  "devDependencies": {
    "npm": "6.14.8",
    "parcel-bundler": "1.12.4",
    "parcel-plugin-static-files-copy": "2.5.0",
    "typescript": "4.0.3"
  },
  "dependencies": {
    "@aspnet/signalr": "1.1.4",
    "phaser": "3.50.0-beta.8"
  },
  "scripts": {
    "start": "parcel ./src/index.html -p 1234 --public-url ./",
    "build": "parcel build ./src/index.html --public-url ./",
    "watch": "parcel watch ./src/index.html --public-url ./",
    "clean": "rm -rf ./dist/* && rm -rf ./.cache",
    "docker-build": "docker build -t olilanz/mini-game ..",
    "docker-push": "docker push olilanz/mini-game"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olilanz/mini-game.git"
  },
  "author": {
    "name": "Oliver and Noah",
    "email": "mini-game@oliverlanz.ch"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/olilanz/mini-game/issues"
  },
  "homepage": "https://github.com/olilanz/mini-game#readme"
}