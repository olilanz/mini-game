{
  "name": "mini-game",
  "version": "0.0.4",
  "description": "Toying around with TypeScript",
  "keywords": [
    "Phaser",
    "Phaser 3",
    "Phaser3",
    "TypeScript",
    "Parcel",
    "Game Examples"
  ],
  "browserslist": [
    "last 3 Chrome version",
    "Firefox ESR",
    "last 3 safari version",
    "last 3 opera version",
    "last 3 edge version"
  ],
  "devDependencies": {
    "npm": "6.7.0",
    "parcel-bundler": "1.11.0",
    "typescript": "3.3.1"
  },
  "dependencies": {
    "@aspnet/signalr": "1.1.0",
    "phaser": "3.16.1"
  },
  "scripts": {
    "start": "parcel ./src/index.html -p 1234 --public-url ./",
    "build": "parcel build ./src/index.html --target browser --public-url ./",
    "watch-build": "parcel watch ./src/index.html --target browser --public-url ./",
    "clean": "rm -rf ./dist/* && rm -rf ./.cache",
    "docker-build": "docker build -t olilanz/mini-game .",
    "docker-push": "docker push olilanz/mini-game"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olilanz/mini-game.git"
  },
  "author": {
    "name": "Oliver and Noah",
    "email": "mini-game@oliverlanz.ch"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/olilanz/mini-game/issues"
  },
  "homepage": "https://github.com/olilanz/mini-game#readme"
}
